#include <stdio.h>
#include <locale.h>

int main() {

  int num1;
  
  printf("Coloque o primeiro número: ");
  scanf("%d", &num1);

  int num2;
  
  printf("Coloque o segundo número: ");
  scanf("%d", &num2);

  if(num1 > num2) {
    printf("O maior número é: %d", num1);
  
}else{
    printf("O maior número é: %d", num2);
}
}
